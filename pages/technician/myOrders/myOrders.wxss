.container { padding: 30rpx; }
.title { font-size: 36rpx; font-weight: bold; margin-bottom: 20rpx; }
.order-item { background: #fff; border: 1px solid #ddd; border-radius: 10rpx; padding: 20rpx; margin-bottom: 20rpx; }
.order-title { font-size: 32rpx; font-weight: bold; margin-bottom: 10rpx; }
.order-info { font-size: 26rpx; color: #666; display: flex; justify-content: space-between; }
.empty { text-align: center; color: #999; font-size: 28rpx; margin-top: 200rpx; }
